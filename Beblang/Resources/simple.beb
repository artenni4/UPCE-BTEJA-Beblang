MODULE Test;

VAR global_a, gcd: INTEGER;

PROCEDURE CalcGcd(a, b: INTEGER) : INTEGER
VAR
  tmp: INTEGER;
BEGIN
  WHILE b # 0
  DO
    tmp := b;
    b := a MOD b;
    a := tmp;
  END;
  
  RETURN a;
END;

BEGIN
  global_a := 72;
  PrintString("Hello, World!\n");
  gcd := CalcGcd(global_a, 42);
  PrintInteger(gcd);
END.