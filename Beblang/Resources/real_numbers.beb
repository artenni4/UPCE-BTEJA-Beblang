MODULE RealNumbers;

IMPORT System;

VAR
  num1, num2: REAL;
  sum, difference, product, quotient: REAL;
BEGIN
  num1 := 5.5;
  num2 := 2.2;
  
  sum := num1 + num2;
  difference := num1 - num2;
  product := num1 * num2;
  quotient := num1 / num2;
  
  PrintLine("Sum: " + REAL_TO_STRING(sum));
  PrintLine("Difference: " + REAL_TO_STRING(difference));
  PrintLine("Product: " + REAL_TO_STRING(product));
  PrintLine("Quotient: " + REAL_TO_STRING(quotient));
  
END.
