MODULE Arrays;

VAR
  f: ARRAY 10 OF ARRAY 10 OF REAL;
  f_i, f_j: INTEGER;

BEGIN
  PrintString("Array test\n");

  f[1][2] := 1.0;
  f[2][3] := 2.0;
  f[4][1] := 5.5;
  
  f_i := 0;
  WHILE f_i < 10
  DO
    f_j := 0;
    WHILE f_j < 10
    DO
      PrintReal(f[f_i][f_j]);
      PrintString(" ");
      f_j := f_j + 1;
    END;
    PrintString("\n");
    f_i := f_i + 1;
  END;
  
  PrintString("Done\n");
END.